        }
     }

    }
    programs {
      program {
        name, "on_show";
        signal, "show";
        source, "";
        action, STATE_SET "visible" 0.0;
        transition, DECELERATE 0.4;
        target, "background";
        after: "animation.in";
      }
      program {
        name, "animation.in";
        action, STATE_SET "animationin" 0.0;
        transition, LINEAR 2.5;
        target, "animation";
        after: "shadow.in";
        after: "bloom.in";
        after: "neon.in";
        after: "footerclip.in";
      }
      program {
        name, "neon.in";
        action, STATE_SET "neonon" 0.0;
        transition, LINEAR 4.0;
        target, "neonlight";
        after: "neon.loop";
      }
      program {
        name, "neon.loop";
        action, STATE_SET "neononloop" 0.0;
        transition, LINEAR 0.000001;
        target, "neonlight";
        after: "neon.in";
      }
      program {
        name, "shadow.in";
        action, STATE_SET "shadowin" 0.0;
        transition, SINUSOIDAL 0.2;
        target, "shadow";
      }
      program {
        name, "bloom.in";
        in: 0.2 0.0;
        action, STATE_SET "bloomon" 0.0;
        transition, ACCELERATE 0.2;
        target, "bloom";
        after: "bloom.out";
        after: "animation.final";
      }
      program {
        name, "footerclip.in";
        action, STATE_SET "footerclipon" 0.0;
        transition, DECELERATE 1.0;
        target, "footerclip";
        after: "loadericon.in";
      }
      program {
        name, "loadericon.in";
        action, STATE_SET "loadericonon" 0.0;
        transition, DECELERATE 0.5;
        target, "loadericon";
        after: "loadericon.anim";
      }
      program {
        name, "loadericon.anim";
        action, STATE_SET "loadericonanim" 0.0;
        transition, ACCELERATE 0.9;
        target, "loadericon";
        after: "loadericon.animout";
      }
      program {
        name, "loadericon.animout";
        action, STATE_SET "loadericonout" 0.0;
        transition, DECELERATE 0.9;
        target, "loadericon";
        after: "loadericon.anim";
      }
      program {
        name, "bloom.out";
        action, STATE_SET "bloomout" 0.0;
        transition, DECELERATE 0.5;
        target, "bloom";
      }
      program {
        name, "animation.final";
        in: 0.1 0.0;
        action, STATE_SET "finalin" 0.0;
        transition, DECELERATE 0.5;
        target, "final";
        after: "reflect.in";
        after: "mirror.in";
      }
      program {
        name, "mirror.in";
        action, STATE_SET "logomirrorin" 0.0;
        transition, DECELERATE 0.9;
        target, "logomirror";
      }
      program {
        name, "reflect.in";
        action, STATE_SET "reflecton" 0.0;
        transition, ACCELERATE 0.5;
        target, "reflect";
        after: "reflect.out";
      }
      program {
        name, "reflect.out";
        action, STATE_SET "default" 0.0;
        transition, DECELERATE 0.0;
        target, "reflect";
        after: "accueil.in";
      }
      program {
        name, "accueil.in";
        action, STATE_SET "accueilon" 0.0;
        transition, DECELERATE 1.0;
        target, "accueil";
        after: "version.in";
      }
      program {
        name, "version.in";
        action, STATE_SET "versionon" 0.0;
        transition, DECELERATE 0.5;
        target, "version";
      }
      //prog pour remettre a l'etat initial lorsque l'element est "cache"
      program {
        name, "animation.reset";
        signal: "hide";
        source: "";
        action, STATE_SET "default" 0.0;
        transition, LINEAR 0.00001;
        target, "animation";
        target, "shadow";
        target, "bloom";
        target, "final";
        target, "reflect";
        target, "accueil";
        target, "neonlight";
        target, "footerclip";
      }
    }
  }
}
