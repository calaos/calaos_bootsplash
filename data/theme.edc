      }

      part {
        name: "final";
        type: IMAGE;
        description {
          state: "default" 0.0;
          visible: 0;
          color: 255 255 255 0;
          image { normal: "final.png"; }
          rel1.to: "logoclip";
          rel2.to: "logoclip";
        }
        description {
          state: "finalin" 0.0;
          visible: 1;
          color: 255 255 255 255;
          image { normal: "final.png"; }
          rel1.to: "logoclip";
          rel2.to: "logoclip";
        }
        description {
          state: "finalout" 0.0;
          visible: 0;
          color: 255 255 255 0;
          image { normal: "final.png"; }
          rel1.to: "logoclip";
          rel2.to: "logoclip";
        }
      }

      part {
        name: "logomirror";
        type: IMAGE;
        description {
          state: "default" 0.0;
          visible: 1;
          color: 255 255 255 0;
          image { normal: "logo_mirror.png"; }
          rel1.to: "logomirrorclip";
          rel2.to: "logomirrorclip";
        }
        description {
          state: "logomirrorin" 0.0;
          visible: 1;
          color: 255 255 255 6;
          image { normal: "logo_mirror.png"; }
          rel1.to: "logomirrorclip";
          rel2.to: "logomirrorclip";
        }
      }

      part {
        name: "bloom";
        type: IMAGE;
        description {
          state: "default" 0.0;
          visible: 0;
          color: 255 255 255 0;
          image { normal: "bloom.png"; }
          rel1.to: "logoclip";
          rel2.to: "logoclip";
        }
        description {
          state: "bloomon" 0.0;
          visible: 1;
          color: 255 255 255 255;
          image { normal: "bloom.png"; }
          rel1.to: "logoclip";
          rel2.to: "logoclip";
        }
        description {
          state: "bloomout" 0.0;
          visible: 0;
          color: 255 255 255 0;
          image { normal: "bloom.png"; }
          rel1.to: "logoclip";
          rel2.to: "logoclip";
        }
      }

      part {
        name: "reflect";
        type: IMAGE;
        description {
          state: "default" 0.0;
          visible: 0;
          color: 255 255 255 100;
          image { normal: "reflect_007.png"; }
          rel1.to: "logoclip";
          rel2.to: "logoclip";
        }
        description {
          state: "reflecton" 0.0;
          visible: 1;
          image {
            normal: "reflect_007.png";
            tween: "reflect_001.png";
            tween: "reflect_002.png";
            tween: "reflect_003.png";
            tween: "reflect_004.png";
            tween: "reflect_005.png";
            tween: "reflect_006.png";
            tween: "reflect_007.png";
          }
          rel1.to: "logoclip";
          rel2.to: "logoclip";
        }
      }

      part {
        name, "accueil";
        type: IMAGE;
        description {
          state, "default" 0.0;
          visible: 0;
          rel1 { relative: 0.505  0.90; offset: -205   -11; }
          rel2 { relative: 0.505  0.90; offset: 204  10; }
          image { normal:"logo_tittle.png";}
          color: 255 255 255 0;
          rel1.to: "logoclip";
          rel2.to: "logoclip";
        }
        description {
          state, "accueilon" 0.0;
          visible: 1;
          rel1 { relative: 0.505  0.90; offset: -205   -11; }
          rel2 { relative: 0.505  0.90; offset: 204  10; }
          image { normal:"logo_tittle.png";}
          color: 255 255 255 90;
          rel1.to: "logoclip";
          rel2.to: "logoclip";
        }
     }

      part {
        name, "loadericon";
        type: IMAGE;
        description {
          state, "default" 0.0;
          visible: 0;
          rel1 { relative: 0.037  0.508; offset: -21   -19; }
          rel2 { relative: 0.037  0.508; offset: 20  18; }
          image { normal:"loader_icon_bg.png";}
          color: 255 255 255 0;
          rel1.to: "footerclip";
          rel2.to: "footerclip";
        }
        description {
          state, "loadericonon" 0.0;
          visible: 1;
          rel1 { relative: 0.037  0.508; offset: -21   -19; }
          rel2 { relative: 0.037  0.508; offset: 20  18; }
          image { normal:"loader_icon_bg.png";}
          color: 255 255 255 255;
          rel1.to: "footerclip";
          rel2.to: "footerclip";
        }
        description {
          state, "loadericonanim" 0.0;
          visible: 1;
          rel1 { relative: 0.037  0.508; offset: -21   -19; }
          rel2 { relative: 0.037  0.508; offset: 20  18; }
          color: 255 255 255 255;
          image {
            normal: "loader_icon_0025.png";
            tween: "loader_icon_0001.png";
            tween: "loader_icon_0002.png";
            tween: "loader_icon_0003.png";
            tween: "loader_icon_0004.png";
            tween: "loader_icon_0005.png";
            tween: "loader_icon_0006.png";
            tween: "loader_icon_0007.png";
            tween: "loader_icon_0008.png";
